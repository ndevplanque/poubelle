<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Admin</title>
    <script defer src="/scripts.js"></script>
</head>
<body>

<% Object.keys(trashes).forEach(key => { %>
    <p>
        <span>Trash <%= key %></span>
        (<span id="trash-<%= key %>-open"></span>, <span id="trash-<%= key %>-full"></span>)
    </p>
    <button onclick="setOpen('<%= key %>', true)">Ouvrir</button>
    <button onclick="setOpen('<%= key %>', false)">Fermer</button>
    <button onclick="setFull('<%= key %>', true)">Remplir</button>
    <button onclick="setFull('<%= key %>', false)">Vider</button>
<% }); %>

</body>
</html>